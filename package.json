{
  "name": "react-redux-tmp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start:dev",
    "start-prod": "better-npm-run start:prod",
    "build-dev": "better-npm-run build:dev",
    "build-prod": "better-npm-run build:prod",
    "analyz": "better-npm-run build:analyz"
  },
  "betterScripts": {
    "start:dev": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development",
        "PORT": 5000,
        "ISBUILD": 0
      }
    },
    "start:prod": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "production",
        "PORT": 5000,
        "ISBUILD": 0
      }
    },
    "build:dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "ISBUILD": 1
      }
    },
    "build:prod": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "ISBUILD": 1
      }
    },
    "build:analyz": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "ISBUILD": 1,
        "ANALYZ": 1
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "axios": "^0.21.1",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime-corejs2": "^7.10.5",
    "babel-loader": "^8.1.0",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
